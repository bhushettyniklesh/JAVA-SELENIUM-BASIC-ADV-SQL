package java1;

public class loppingconditions {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
   for(int i=1;i<11;i++)
   {
   System.out.println(i);
   
	}
   {
	   System.out.println("ended"); 
   }
   for (int j=10;j>0;j--)
   {
	   System.out.println(j);
   }
   for (int h=10;h>0;h-=2)
   {
	   System.out.println(h);
   }
   {
	   System.out.println("end all and continue tables");
   }
   int num4=5;
   for(int k=1;k<11;k++) {
	   System.out.println(num4*k);
   }
	}
}
