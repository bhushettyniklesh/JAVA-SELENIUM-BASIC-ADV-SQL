package java1;

public class loopingexample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
       int num=1;
       while (num<11) {
    	   System.out.println(num);
    	   num =num+1;
    	   
    	   int num2=10;{
    		   while(num2>1);
    	   System.out.println(num2);
    	   num=num-1;
       }}
	}

}
