package javaoops;

public class inherhits extends persons {
public int ssn;

public inherhits() {
	System.out.println("inside para default");
}
public inherhits(String name,int age,String CountryName,int ssn) {
	System.out.println("para cons");
	this.name=name;
	this.age=age;
	persons.CountryName=CountryName;
	this.ssn=ssn;
	
}
}
