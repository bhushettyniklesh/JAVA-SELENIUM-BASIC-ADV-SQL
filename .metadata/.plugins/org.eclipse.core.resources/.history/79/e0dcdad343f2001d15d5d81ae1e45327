package java1;

public class arrays {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
   int[] arry1= {29,30,34,57,54};
   System.out.println("print length "+ arry1.length);
   System.out.println("print first "+ arry1[3]);
   System.out.println("print last "+ arry1[arry1.length-1]);
   
   String[] num2= {"mon","tues","wed","thus","friday"};
   System.out.println("print third "+ num2.length);
   System.out.println("print 3 string "+ num2[2]);
   System.out.println("print last "+ num2[num2.length-1]);
   
   String s1="i love my indian culture and flag";
   String[] arry4=s1.split(" ");
   System.out.println("world count "+arry4.length);
	}

}
